    "@types/ini": "^1.3.29",
    "@types/jest": "^21.1.5",
    "@types/js-yaml": "^3.9.1",
    "dotenv": "^4.0.0",
    "ini": "^1.3.4",
    "jest": "^21.2.1",
    "js-yaml": "^3.10.0",
    "rimraf": "^2.6.2",
    "toml": "^2.3.3",
    "ts-jest": "^21.1.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.5.3"
